/*=======================================================
                     SCREEN 7 Tab content -3
========================================================*/
.Documents_Tabs {border-radius: 10px;filter: drop-shadow(0 0 6.5px rgba(74,74,74,0.1));background-color: #ffffff;border: 1px solid #dfe2e5;}
button.new_file_btn {width: 141px;border-radius: 5px;    color: #06b4ff;background-color: #fff;border: 1px solid #06b4ff;font-size: 16px;letter-spacing: 0px;line-height: 40px;font-family: Rubik Regular;text-align: center;margin: 0 30px;padding: 0 10px;}
button.new_file_btn svg.feather-folder-plus {float: left;margin-top: 11px;}
button.new_file_btn:hover, 
button.new_file_btn:focus {background-color: #06b4ff;    color: #ffffff;}
.Documents_List {text-align: right;padding: 15px 0;}
